{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jasmine": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "import/extensions": 0,
    "no-console": 0,
    "no-shadow": 0,
    "no-undef": 0,
    "no-unused-vars": 0,
    "no-use-before-define": 0,
    "no-underscore-dangle": 0,
    "no-restricted-globals": 0,
    "no-prototype-builtins": 0,
    "consistent-return": 0,
    "import/prefer-default-export": 0,
    "prefer-destructuring": 0,
    "linebreak-style": 0
  },
  "plugins": ["jasmine"]
}
